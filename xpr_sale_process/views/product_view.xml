<openerp>
	<data>
		<record id="product_xprima_sale_process_view_form" model="ir.ui.view">
			<field name="name">product.form.xprima_sale_process</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_only_form_view"/>
			<field name="arch" type="xml">
				<xpath expr="/form/sheet/notebook/page[@string='Sales']/group[@name='sale']/group[@name='sale_condition']" position="after">
					<group>
						<field name="approver_groups" />
					</group>
					<group>
						<field name="availability_groups" />
					</group>
				</xpath>
			</field>
		</record>
	</data>
</openerp>
